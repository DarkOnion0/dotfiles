{
  // LSP
  "languageserver": {
    "nix": {
      "command": "rnix-lsp",
      "filetypes": [
        "nix"
      ]
    }
  },
  "suggest.completionItemKindLabels": {
    "text": "",
    "method": "ƒ",
    "function": "ﬦ",
    "constructor": "",
    "variable": "",
    "class":"",
    "interface": "ﰮ",
    "module": "",
    "property": "",
    "unit": "",
    "value": "",
    "enum": "了",
    "keyword": "",
    "snippet": "﬌",
    "color": "",
    "file": "",
    "folder": "",
    //"enummember" : "",
    "constant": "",
    "struct": ""
  },

  // ALE config
  "diagnostic.displayByAle": false,

  // Coc internal settings
  "coc.preferences.extensionUpdateCheck": "daily",

  // Coc-explorer
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,

  // Coc-git
  "git.enableGutters": true,
  "git.addGBlameToVirtualText": true,

  // Fix Fuzzy search with coc
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit"
  }

}
